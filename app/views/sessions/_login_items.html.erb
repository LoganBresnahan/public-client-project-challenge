<ul class="nav nav-tabs">
<% if !session[:user_id] %>
  <li><%= link_to("Login", new_session_path ) %></li>
  <li><%= link_to("Register", new_user_path ) %></li>
<% else %>
  <li><%="Welcome, #{current_user.email}"%></li>
  <li><%=link_to("Logout", session_path(session[:user_id]), method: :delete ) %></li>
<% end %>
</ul>
