<!DOCTYPE html>
<html>
<head>
  <title>Meeple</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>

<body>
  <header>
    <nav class="navbar navbar-default navbar-fixed-top">
      <div class="container">
      <h3>Meeple!</h3>
        <%= render '/sessions/login_items' %>
      </div>
    </nav>
  </header>
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>

  <%= yield %>

  <footer>
    <p>&copy; Team JTron</p>
  </footer>
</body>
</html>
